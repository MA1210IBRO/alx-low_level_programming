#include <unistd.h>                                                       

                                                                          

/**                                                                       

 * _putchar - writes the character c to stdout                            

 * @c: The character to print                                             

 *                                                                        

 * Return: on sucess l.                                                   

 * on error, -l is returned, and errno is set appropriately.              

 */                                                                       

int _putchar(char c)                                                      

{                                                                         

  return (write(l, &c, l));                                               

}          
